{
    "name": "kanban-api",
    "version": "1.0.0",
    "description": "API for KanBan application",
    "main": "main.ts",
    "scripts": {
        "test": "jest --silent --noStackTrace --runInBand",
        "test:watch": "jest --silent --noStackTrace --runInBand --watchAll",
        "test:coverage": "jest --silent --noStackTrace --runInBand --coverage",
        "dev": "ts-node-dev --transpile-only --respawn ./src/main.ts",
        "start": "ts-node src/index.ts",
        "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js"
    },
    "keywords": [
        "kanban",
        "api",
        "node"
    ],
    "author": "pviniciusm",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.26.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "dotenv-safe": "^8.2.0",
        "express": "^4.17.3",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "sqlite3": "^5.0.2",
        "typeorm": "^0.2.45",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/dotenv-safe": "^8.1.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^16.11.10",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8.3.4",
        "jest": "^27.5.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-node": "10.7.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "4.5.2"
    }
}
